{
  "name": "buidler-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "task:accounts": "npx buidler accounts",
    "clean": "npx buidler clean && rm -rf cache coverage node_modules yarn.lock coverage.json",
    "deploy": "npx buidler run scripts/deploy.js",
    "compile": "npx buidler compile",
    "test": "npx buidler test",
    "test:gas": "scripts/run-gas-reporter.sh",
    "test:gas:token": "scripts/run-gas-reporter.sh test/Token/*",
    "test:gas:counter": "scripts/run-gas-reporter.sh test/Counter/*",
    "test:token": "npx buidler test test/Token/*",
    "test:counter": "npx buidler test test/Counter/*",
    "test:coverage": "npx buidler coverage --network coverage",
    "test:coverage:token": "npx buidler coverage --testfiles test/Token --solcoverjs test/Token/.solcover.js --network coverage",
    "test:coverage:counter": "npx buidler coverage --testfiles test/Counter --solcoverjs test/Counter/.solcover.js --network coverage",
    "lint:sol": "npx buidler check",
    "doc": "yarn run solidity-docgen"
  },
  "keywords": [],
  "author": "",
  "license": "GPL-3.0",
  "devDependencies": {
    "@nomiclabs/buidler": "^1.4.2",
    "@nomiclabs/buidler-ethers": "^2.0.0",
    "@nomiclabs/buidler-etherscan": "^1.3.3",
    "@nomiclabs/buidler-solhint": "^1.3.3",
    "@nomiclabs/buidler-waffle": "^2.0.0",
    "@openzeppelin/test-helpers": "^0.5.6",
    "buidler-deploy": "^0.5.6",
    "buidler-gas-reporter": "^0.1.3",
    "buidler-spdx-license-identifier": "^1.0.2",
    "chai": "^4.2.0",
    "dotenv": "^8.2.0",
    "eth-gas-reporter": "^0.2.17",
    "ethereum-waffle": "^3.0.2",
    "ethers": "^5.0.7",
    "mocha": "^8.0.1",
    "prettier": "^2.0.5",
    "prettier-plugin-solidity": "^1.0.0-alpha.55",
    "solidity-coverage": "^0.7.9"
  }
}
